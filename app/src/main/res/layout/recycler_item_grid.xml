<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/event_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="Event"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/event_text"
        android:id="@+id/match_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="Match"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/match_text"
        android:id="@+id/team_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="Team"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/team_text"
        android:id="@+id/autoc_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="Auto Comment"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/autoc_text"
        android:id="@+id/LAgrid_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="0 0 0 0 0 0 0 0 0"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/LAgrid_text"
        android:id="@+id/CAgrid_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="0 0 0 0 0 0 0 0 0"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/CAgrid_text"
        android:id="@+id/RAgrid_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="0 0 0 0 0 0 0 0 0"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/RAgrid_text"
        android:id="@+id/teleop_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="Teleop Comment"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/teleop_text"
        android:id="@+id/LTgrid_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="0 0 0 0 0 0 0 0 0"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/LTgrid_text"
        android:id="@+id/CTgrid_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="0 0 0 0 0 0 0 0 0"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/CTgrid_text"
        android:id="@+id/RTgrid_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="0 0 0 0 0 0 0 0 0"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/RTgrid_text"
        android:id="@+id/docking_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="Dock"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/docking_text"
        android:id="@+id/dockTime_text"
        android:padding="10dp"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:text="Dock Time"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/backgroundColorGrid"
        android:layout_below="@id/event_text"/>
</RelativeLayout>